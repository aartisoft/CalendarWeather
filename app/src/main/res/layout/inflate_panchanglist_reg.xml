<?xml version="1.0" encoding="utf-8"?>

<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"

    android:layout_margin="5dp"
    android:elevation="100dp"
    app:cardBackgroundColor="?attr/cardBackgroundColor"


    >

    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"

        >

        <TextView

            android:id="@+id/dateTxt"
            style="@style/TextAppearance.Medium"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:background="@color/dellYellow"


            android:gravity="center"
            android:padding="2dp"
            android:text="Date"
            android:textColor="@color/myColor6"
            android:textStyle="bold" />


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:background="@drawable/borderpanchang"
                android:baselineAligned="false"
                android:orientation="horizontal"
                android:padding="2dp">


                <TextView
                    android:id="@+id/txt3"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Sunrise" />

                <View
                    android:layout_width="0.2dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="start"
                    android:background="?attr/divider" />

                <TextView
                    android:id="@+id/txt4"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Sunset" />


            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:background="@drawable/borderpanchang"
                android:baselineAligned="false"
                android:orientation="horizontal"
                android:padding="2dp">


                <TextView

                    android:id="@+id/txt5"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Noon" />


                <View
                    android:layout_width="0.2dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="start"
                    android:background="?attr/divider" />


                <TextView

                    android:id="@+id/txt6"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Day Length" />


            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:background="@drawable/borderpanchang"
                android:baselineAligned="false"
                android:orientation="horizontal"
                android:padding="2dp">


                <TextView

                    android:id="@+id/txt7"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Moonrise" />


                <View
                    android:layout_width="0.2dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="start"
                    android:background="?attr/divider" />


                <TextView

                    android:id="@+id/txt8"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Moonset" />


            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:background="@drawable/borderpanchang"
                android:baselineAligned="false"
                android:orientation="horizontal"
                android:padding="2dp">


                <TextView

                    android:id="@+id/txt9"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Midnight" />


                <View
                    android:layout_width="0.2dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="start"
                    android:background="?attr/divider" />


                <TextView

                    android:id="@+id/txt10"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Night Length" />


            </LinearLayout>


        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_gravity="start"
            android:layout_margin="5dp"
            android:background="?attr/divider" />


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:background="@drawable/borderpanchang"
                android:baselineAligned="false"
                android:orientation="horizontal"
                android:padding="2dp">


                <TextView

                    android:id="@+id/txt16"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Tithi" />


                <View
                    android:layout_width="0.2dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="start"
                    android:background="?attr/divider" />


                <TextView

                    android:id="@+id/txt18"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Nakshetra" />


            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:background="@drawable/borderpanchang"
                android:baselineAligned="false"
                android:orientation="horizontal"
                android:padding="2dp"
                android:visibility="visible">


                <TextView

                    android:id="@+id/txt19"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="SunSign" />


                <View
                    android:layout_width="0.2dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="start"
                    android:background="?attr/divider" />


                <TextView

                    android:id="@+id/txt20"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="MoonSign" />


            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:background="@drawable/borderpanchang"
                android:baselineAligned="false"
                android:orientation="horizontal"
                android:padding="2dp">


                <TextView

                    android:id="@+id/txt21"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Joga" />


                <View
                    android:layout_width="0.2dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="start"
                    android:background="?attr/divider" />


                <TextView

                    android:id="@+id/txt22"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Karana" />


            </LinearLayout>


        </LinearLayout>

        <View
            android:id="@+id/divider4"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_gravity="start"
            android:layout_margin="5dp"
            android:background="?attr/divider" />


        <LinearLayout
            android:id="@+id/cntr4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:baselineAligned="false"
            android:orientation="vertical">

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:background="@drawable/borderpanchang"
                android:orientation="vertical"
                android:padding="2dp">


                <TextView
                    android:id="@+id/jogin"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Jogin" />

                <TextView
                    android:id="@+id/jogin_help"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/myColor4"
                    android:text="@string/l_jogin_help" />


            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:background="@drawable/borderpanchang"
                android:baselineAligned="false"
                android:orientation="horizontal"
                android:padding="2dp">


                <TextView
                    android:id="@+id/ghataBar"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="start"
                    android:text="ghataBar" />


                <View
                    android:layout_width="0.2dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="start"
                    android:background="?attr/divider" />


                <TextView

                    android:id="@+id/ghataChandra"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="ghataChandra" />


            </LinearLayout>


            <TextView
                android:id="@+id/tarashuddhi"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:background="@drawable/borderpanchang"
                android:padding="2dp"
                android:text="tarashuddhi" />


            <TextView
                android:id="@+id/chandrashuddhi"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:background="@drawable/borderpanchang"
                android:padding="2dp"
                android:text="chandrashuddhi" />


        </LinearLayout>


        <View
            android:id="@+id/divider5"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_gravity="start"
            android:layout_margin="5dp"
            android:background="?attr/divider" />


        <LinearLayout
            android:id="@+id/cntr5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"

            android:visibility="visible">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:background="@drawable/borderpanchang"
                android:baselineAligned="false"
                android:orientation="horizontal"
                android:padding="2dp">


                <TextView

                    android:id="@+id/puskar"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="start"
                    android:text="puskar Bela" />


                <View
                    android:layout_width="0.2dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="start"
                    android:background="?attr/divider" />


                <TextView

                    android:id="@+id/shraddha"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="shraddha" />


            </LinearLayout>


            <TextView
                android:id="@+id/rajadarsan"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:background="@drawable/borderpanchang"
                android:padding="2dp"
                android:text="rajadarsan" />


            <TextView
                android:id="@+id/food"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:background="@drawable/borderpanchang"
                android:padding="2dp"
                android:text="Food" />


        </LinearLayout>

        <View
            android:id="@+id/divider6"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_gravity="start"
            android:layout_margin="5dp"
            android:background="?attr/divider" />

        <LinearLayout
            android:id="@+id/cntr6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/sarana"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:background="@drawable/borderpanchang"
                android:padding="2dp"
                android:text="Sarana Panchak" />
            <TextView
                android:id="@+id/grahakuta"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:background="@drawable/borderpanchang"
                android:gravity="start"
                android:padding="2dp"

                android:text="Grahakuta" />
            <TextView
                android:id="@+id/fest1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:background="@drawable/borderpanchang"
                android:gravity="start"
                android:padding="2dp"

                android:text="Festival" />


        </LinearLayout>






        <View
            android:id="@+id/divider8"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_gravity="start"
            android:layout_margin="5dp"
            android:background="?attr/divider"
            android:visibility="gone"
             />

        <LinearLayout
            android:id="@+id/cntr7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:orientation="vertical"

            android:visibility="visible">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:baselineAligned="false">

                <ImageView

                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_toStartOf="@+id/shubhakama"
                    android:src="@drawable/ic_festival"
                    android:tint="@color/myColor6" />

                <TextView

                    android:id="@+id/shubhakama"

                    style="@style/TextAppearance.Medium"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:text="@string/l_auspicious_work"

                    android:textColor="@color/myColor6"
                    android:textStyle="bold" />
            </RelativeLayout>

            <TextView
                android:id="@+id/shubhakamalist"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:layout_margin="2dp"
                android:background="@drawable/borderpanchang"
                android:gravity="start"

                android:padding="2dp"
                android:scrollHorizontally="false"
                android:text="not found" />

        </LinearLayout>
        <View
            android:id="@+id/divider9"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_gravity="start"
            android:layout_margin="5dp"
            android:background="?attr/divider"
            android:visibility="visible"
            />
        <LinearLayout
            android:visibility="gone"
            android:id="@+id/cntr8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:baselineAligned="false"
            android:orientation="vertical">

            <TextView
                android:id="@+id/lagna_title"
                style="@style/TextAppearance.Medium"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:ellipsize="end"
                android:gravity="center"
                android:text="@string/l_lagna_title"
                android:textColor="@color/myColor2"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/lagna_timings"
                style="@style/TextAppearance.Small"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:ellipsize="end"
                android:gravity="center"
                android:text="@string/l_lagna_at_sunrise"
                android:textColor="@color/myColor2" />
            <TextView
                android:id="@+id/lagna1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"

                android:background="@drawable/borderpanchang"
                android:gravity="start"
                android:padding="2dp"
                android:text="lagna1" />



        </LinearLayout>


        <View
            android:id="@+id/divider7"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_gravity="start"
            android:layout_margin="5dp"
            android:background="?attr/divider" />


        <LinearLayout
            android:visibility="visible"
            android:id="@+id/cntr9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:baselineAligned="false"
            android:orientation="vertical">

            <TextView
                android:id="@+id/muhurta_title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:ellipsize="end"
                android:gravity="center"
                android:text="@string/l_muhurta_title"
                android:textColor="@color/myColor3"
                />

            <TextView
                android:id="@+id/muhurta"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"

                android:background="@drawable/borderpanchang"
                android:gravity="start"
                android:padding="2dp"
                android:text="muhurta" />



        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_gravity="start"
            android:layout_margin="5dp"
            android:background="?attr/divider" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:baselineAligned="false"
            android:orientation="vertical">


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:baselineAligned="false">

                <ImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_toLeftOf="@+id/auspicious_timings"
                    android:src="@drawable/ic_festival"
                    android:tint="@color/myColor6" />

                <TextView
                    android:id="@+id/auspicious_timings"
                    style="@style/TextAppearance.Medium"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"

                    android:ellipsize="end"
                    android:text="@string/l_auspicious_timings"

                    android:textColor="@color/myColor6"
                    android:textStyle="bold" />

            </RelativeLayout>


            <TextView
                android:id="@+id/amritaBela"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:background="@drawable/borderpanchang"
                android:gravity="start"
                android:padding="2dp"
                android:text="Brahma Bela" />


            <TextView
                android:id="@+id/mahendarBela"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:background="@drawable/borderpanchang"
                android:gravity="start"
                android:padding="2dp"
                android:text="mahendarBela" />


            <TextView
                android:id="@+id/txt24"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:background="@drawable/borderpanchang"
                android:gravity="start"
                android:padding="2dp"
                android:text="Abhjit Bela" />


            <TextView
                android:id="@+id/txt25"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:background="@drawable/borderpanchang"
                android:gravity="start"
                android:padding="2dp"
                android:text="Brahma Bela" />


        </LinearLayout>



        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_gravity="start"
            android:layout_margin="5dp"
            android:background="?attr/divider" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:baselineAligned="false"
            android:orientation="vertical">

            <TextView
                android:id="@+id/inauspicious_timings"
                style="@style/TextAppearance.Medium"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:ellipsize="end"
                android:gravity="center"
                android:text="@string/l_inauspicious_timings"
                android:textColor="@color/myColor7"
                android:textStyle="bold" />


            <TextView
                android:id="@+id/txt31"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:background="@drawable/borderpanchang"
                android:gravity="start"
                android:padding="2dp"
                android:text="Vara Bela" />


            <TextView
                android:id="@+id/txt32"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:background="@drawable/borderpanchang"
                android:gravity="start"
                android:padding="2dp"
                android:text="Kala Bela" />


            <TextView
                android:id="@+id/txt33"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:background="@drawable/borderpanchang"
                android:gravity="start"
                android:padding="2dp"
                android:text="Kala Ratri" />


            <TextView
                android:id="@+id/txt26"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:background="@drawable/borderpanchang"
                android:gravity="start"
                android:padding="2dp"
                android:text="Vajyama Bela" />


            <TextView
                android:id="@+id/txt27"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:background="@drawable/borderpanchang"
                android:gravity="start"
                android:padding="2dp"
                android:text="Durmuhurta Bela" />


            <TextView
                android:id="@+id/txt28"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:background="@drawable/borderpanchang"
                android:gravity="start"
                android:padding="2dp"
                android:text="Rahu Bela" />


            <TextView
                android:id="@+id/txt29"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:background="@drawable/borderpanchang"
                android:gravity="start"
                android:padding="2dp"
                android:text="Gulika Bela" />


            <TextView
                android:id="@+id/txt30"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:background="@drawable/borderpanchang"
                android:gravity="start"
                android:padding="2dp"
                android:text="Yama Bela" />


        </LinearLayout>


    </LinearLayout>


</androidx.cardview.widget.CardView>